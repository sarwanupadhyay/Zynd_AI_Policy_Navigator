# Server Configuration
PORT=5000
NODE_ENV=development

# Zynd Agent Credentials
# Get these from your Zynd Dashboard after creating agents

# Policy Interpreter Agent
POLICY_INTERPRETER_DID=did:zynd:ab12...9f
POLICY_INTERPRETER_SEED=your_secure_seed_here
POLICY_INTERPRETER_CREDENTIAL={"id":"vc:policy-interpreter","type":"AgentCredential","issuer":"did:zynd:issuer"}

# Eligibility Verifier Agent
ELIGIBILITY_VERIFIER_DID=did:zynd:cd34...7e
ELIGIBILITY_VERIFIER_SEED=your_secure_seed_here
ELIGIBILITY_VERIFIER_CREDENTIAL={"id":"vc:eligibility-verifier","type":"AgentCredential","issuer":"did:zynd:issuer"}

# Citizen Agent
CITIZEN_AGENT_DID=did:zynd:ef56...3d
CITIZEN_AGENT_SEED=your_secure_seed_here
CITIZEN_AGENT_CREDENTIAL={"id":"vc:citizen","type":"AgentCredential","issuer":"did:zynd:issuer"}

# Application Guide Agent
APPLICATION_GUIDE_DID=did:zynd:gh78...1c
APPLICATION_GUIDE_SEED=your_secure_seed_here
APPLICATION_GUIDE_CREDENTIAL={"id":"vc:application-guide","type":"AgentCredential","issuer":"did:zynd:issuer"}

# MQTT Configuration
MQTT_BROKER=mqtt://broker.zynd.ai:1883

# Optional: For production deployment
# MQTT_USERNAME=your_username
# MQTT_PASSWORD=your_password